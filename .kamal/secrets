SECRETS=$(kamal secrets fetch --adapter doppler --from campsite-sync-server/prd KAMAL_REGISTRY_PASSWORD KAMAL_REGISTRY_USER_NAME API_BASE_URL)

KAMAL_REGISTRY_USER_NAME=$(kamal secrets extract KAMAL_REGISTRY_USER_NAME $SECRETS)
KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD $SECRETS)
API_BASE_URL=$(kamal secrets extract API_BASE_URL $SECRETS)